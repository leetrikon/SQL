CREATE DATABASE laba1
use laba1;

CREATE TABLE S 
( 
  	nomer_postavshika VARChar(6),
  	surname VARCHAR(20),
	rating int,
    city VARCHAR(20),
);

CREATE TABLE P
( 
  	nomer_detali VARChar(6),
  	name VARCHAR(20),
	color VARchAR(20),
    weight INT,
    city VARCHAR(20),
);

CREATE TABLE J
( 
  	nomer_izdelia VARChar(6),
  	name VARCHAR(20),
    city VARCHAR(20),
);

CREATE TABLE SPJ
(
  	nomer_postavshika VARCHAR(6),
  	nomer_detali VARCHAR(6),
	nomer_izdelia VARCHAR(6),
    quantity INT,
);
